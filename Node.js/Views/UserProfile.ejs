<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>BasaBari.com | Collapsed Sidebar Layout</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.7 -->
  <link rel="stylesheet" href="/contents/bower_components/bootstrap/dist/css/bootstrap.min.css">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="/contents/bower_components/font-awesome/css/font-awesome.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="/contents/bower_components/Ionicons/css/ionicons.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="/contents/dist/css/AdminLTE.min.css">
  <!-- AdminLTE Skins. Choose a skin from the css/skins
       folder instead of downloading all of them to reduce the load. -->
  <link rel="stylesheet" href="/contents/dist/css/skins/_all-skins.min.css">

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->

  <!-- Google Font -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">

</head>
<!-- ADD THE CLASS sidebar-collapse TO HIDE THE SIDEBAR PRIOR TO LOADING THE SITE -->
<body class="hold-transition skin-blue sidebar-collapse sidebar-mini">
<!-- Site wrapper -->
<div class="wrapper">



   <%- include partial/UserHeaderWithSidebar.ejs %>

  <!-- =============================================== -->



  <!-- =============================================== -->

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content">
 		<table border="0" width="100%">


	<tr height="600">
		<td width="1%"></td>
		<td valign="top" align="center" width="30%">
			<div class="box box-primary">
            <div class="box-body box-profile">
              <img class="profile-user-img img-responsive img-circle" src="/contents/image/User/<%=result[0].Name%>.jpg" alt="User profile picture">

              <h3 class="profile-username text-center"><%=result[0].Name%></h3><br>

                <ul class="list-group list-group-unbordered">
                 <li class="list-group-item">
                   <b class="pull-left">Joining Date</b> <a class="pull-right"><%=result[0].JoiningDate.getDate()%>/<%=result[0].JoiningDate.getMonth()%>/<%=result[0].JoiningDate.getFullYear()%></a><br>
                 </li>
                 <li class="list-group-item">
                    <b class="pull-left">Phone#</b> <a class="pull-right"><%=result[0].Phone%></a><br>
                 </li>
                <li class="list-group-item">
                  <b class="pull-left">Email</b> <a class="pull-right"><%=result[0].Email%></a><br>
                </li>
                <li class="list-group-item">
                  <b class="pull-left">Points</b> <a class="pull-right"><%=result[0].Points%></a><br>
                </li>
                <li class="list-group-item">
                  <b class="pull-left">Address</b> <a class="pull-right">Kuril,Dhaka</a><br>
                </li>
              </ul>

              <a href="UserSettings" class="btn btn-primary btn-block"><b>Edit Profile</b></a>
            </div>
            <!-- /.box-body -->
          </div>

		</td>
		<td width="5%"></td>
		<td width="65%" valign="top">
      <h1>All Post</h1><br/><br/>
      <table cellspacing="40" width="100%">
      <tr>
        <td width="300">
            <a href="/AddPost"><img width="300" height="200" src="/Contents/image/addPost.png" class="img"></a>
            <br><br><center><a href="/AddPost"><button class="btn btn-primary">Add Post</button></a></center>
        </td>
        <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
        <%
          var count = 1;
          posts.forEach(function(item) { count++; %>

          <td width="300" height="200">
            <a href="/HouseDetails/<%=item.Id%>"><img width="300" height="200" src="/Contents/image/house/<%=item.TitleImage%>" class="img"></a>
           <br>

          &nbsp; Total View: <%=item.TotalView%>

          <br/>
            &nbsp; Tk <%=item.Rent%>/month
          <br/>
            &nbsp; <%=item.Location%>
                  <br/>
            &nbsp; <%=item.BedRoom%> Bedroom, <%=item.Washroom%> Washroom
          </td>
          <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
          <%
            if(count%3==0){%>
              </tr><tr height="30px"></tr>
            <% count=0; }})%>


        </table>
		</td>
	</tr>

</table>
</section>
  </div>
  <!-- /.content-wrapper -->

  <footer class="main-footer">
    <div class="pull-right hidden-xs">
      <b>Version</b> 2.4.0
    </div>
    <strong>Copyright &copy; 2014-2016 <a href="https://adminlte.io">Almsaeed Studio</a>.</strong> All rights
    reserved.
  </footer>

  <!-- /.control-sidebar -->
  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->
  <div class="control-sidebar-bg"></div>
</div>
<!-- ./wrapper -->

<!-- jQuery 3 -->
<script src="/contents/bower_components/jquery/dist/jquery.min.js"></script>
<!-- Bootstrap 3.3.7 -->
<script src="/contents/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
<!-- SlimScroll -->
<script src="/contents/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
<!-- FastClick -->
<script src="/contents/bower_components/fastclick/lib/fastclick.js"></script>
<!-- AdminLTE App -->
<script src="/contents/dist/js/adminlte.min.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="/contents/dist/js/demo.js"></script>
</body>
</html>
